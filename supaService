import {createClient} from '@supabase/supabase-js';

const supabaseUrl = 'https://deecdrkolvumzmudsiho.supabase.co';
const supabaseKey = 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImRlZWNkcmtvbHZ1bXptdWRzaWhvIiwicm9sZSI6ImFub24iLCJpYXQiOjE2OTQ1NzQ5MDYsImV4cCI6MjAxMDE1MDkwNn0.CgiYcsfaM3wU_eFtQwpNWzqhpp8lLUQUN4eBiKId0K4';
const supabase = createClient(supabaseUrl, supabaseKey);

async function fetchLoginInformation() {
    try {
      const { data, error } = await supabase
        .from('Users')
        .select('*')
        .order('created_at', { ascending: true });
  
      if (error) {
        console.error('Error fetching login information:', error);
        return;
      }
  
      // 'data' contains the retrieved login information
      console.log('Login Information:', data);
    } catch (error) {
      console.error('Error fetching login information:', error);
    }
  }

  export {fetchLoginInformation}
